{
    "version": 8,
    "configurePresets": [
        {
            "name": "Donovan",
            "displayName": "GCC 14.1.1 x86_64-redhat-linux",
            "description": "Using compilers: C = /usr/bin/gcc, CXX = /usr/bin/g++",
            "binaryDir": "${sourceDir}/build",
            "cacheVariables": {
                "CMAKE_INSTALL_PREFIX": "${sourcet  Dir}/out/install/${presetName}",
                "CMAKE_C_COMPILER": "/usr/bin/gcc",
                "CMAKE_CXX_COMPILER": "/usr/bin/g++",
                "CMAKE_BUILD_TYPE": ""
            }
        }
    ]
}